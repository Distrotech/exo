#: exo-csource/main.c:230
#, c-format
msgid "Usage: %s [options] [file]\n"
msgstr "使用法: %s [オプション] [ファイル]\n"

#: exo-csource/main.c:231
#, c-format
msgid "       %s [options] --build-list [[name file]...]\n"
msgstr "       %s [オプション] --build-list [[名前 ファイル]...]\n"

#: exo-csource/main.c:233
msgid "  -h, --help        Print this help message and exit\n"
msgstr "  -h, --help        このヘルプメッセージを表示して終了します\n"

#: exo-csource/main.c:234
msgid "  -v, --version     Print version information and exit\n"
msgstr "  -v, --version     バージョン情報を表示して終了します\n"

#: exo-csource/main.c:235
msgid "  --extern          Generate extern symbols\n"
msgstr "  --extern          extern シンボルを生成します\n"

#: exo-csource/main.c:236
msgid "  --static          Generate static symbols\n"
msgstr "  --static          static シンボルを生成します\n"

#: exo-csource/main.c:237
msgid "  --name=identifier C macro/variable name\n"
msgstr "  --name=identifier C マクロ/変数名\n"

#: exo-csource/main.c:238
msgid "  --build-list      Parse (name, file) pairs\n"
msgstr "  --build-list      解析する (名前, ファイルの) ペアです\n"

