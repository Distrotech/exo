msgid "       exo-open --launch TYPE [PARAMETERs...]"
msgstr "       exo-open --launch TYP [PARAMETRY...]"

#: ../exo-open/main.c:46
msgid "  -h, --help                          Print this help message and exit"
msgstr "  -h, --help                          Zobrazí tuto nápovědu"

#: ../exo-open/main.c:47
